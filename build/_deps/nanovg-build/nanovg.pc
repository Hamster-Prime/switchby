prefix=/usr/local
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include/nanovg

Name: nanovg
Description: Antialiased 2D vector drawing library on top of OpenGL for UI and visualizations
Version: 0.1
Requires: glew egl
Libs: -L${libdir} -lnanovg -lGLU -lGL -lm
Cflags: -I${includedir}
